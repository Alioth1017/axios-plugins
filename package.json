{
    "name": "axios-plugins",
    "version": "0.0.1-dev.9",
    "description": "[开发中] 用最小的侵入性, 为 axios 扩展更多的插件能力 (防抖、节流 等等)",
    "keywords": [
        "axios",
        "plugins"
    ],
    "license": "MIT",
    "author": {
        "name": "halo951",
        "url": "https://github.com/halo951"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/halo951/axios-plugins.git"
    },
    "bugs": {
        "url": "https://github.com/halo951/axios-plugins/issues"
    },
    "sideEffects": false,
    "files": [
        "src",
        "dist"
    ],
    "exports": {
        ".": {
            "types": "./dist/typings/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        },
        "./core": {
            "types": "./dist/typings/core.d.ts",
            "require": "./dist/core.js",
            "import": "./dist/core.mjs"
        },
        "./plugins/*": {
            "types": "./dist/typings/plugins/*.d.ts",
            "require": "./dist/plugins/*.js",
            "import": "./dist/plugins/*.mjs"
        }
    },
    "typesVersions": {
        "*": {
            "core": [
                "dist/typings/core.d.ts"
            ],
            "plugins/*": [
                "dist/typings/plugins/*.d.ts"
            ]
        }
    },
    "engines": {
        "node": ">=16"
    },
    "scripts": {
        "clean": "rimraf dist && rimraf typings",
        "build": "rollup --config ./rollup.config.ts --configPlugin typescript2",
        "lint:fix": "prettier -w **.ts **.md **.json",
        "test": "jest"
    },
    "peerDependencies": {
        "axios": "*"
    },
    "devDependencies": {
        "@atomico/rollup-plugin-sizes": "^1.1.4",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.1.0",
        "@rollup/plugin-terser": "^0.4.3",
        "@types/glob": "^8.1.0",
        "@types/jest": "^29.5.1",
        "@types/node": "^18.15.11",
        "@types/qs": "^6.9.7",
        "axios": "^1.4.0",
        "glob": "^10.2.7",
        "jest": "^29.5.0",
        "nock": "^13.3.1",
        "prettier": "^2.8.8",
        "rimraf": "^3.0.2",
        "rollup": "^3.23.0",
        "rollup-plugin-typescript2": "^0.34.1",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "tslib": "^2.5.3",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "axios-logger": "^2.6.2",
        "klona": "^2.0.6",
        "qs": "^6.11.2"
    }
}
